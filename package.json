    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-mysql-session": "^3.0.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.15.0",
    "mongoose": "^8.5.1",
    "mysql2": "^3.14.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
